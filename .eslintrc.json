{
   "env": {
      "browser": true,
      "es2021": true,
      "node": true
   },
   "extends": "airbnb-base",
   "overrides": [],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "rules": {
      "linebreak-style": 0,
      "eol-last": ["error", "never"],
      "indent": ["error", 3],
      "no-console": "off",
      "no-unused-vars": "off"
   }
}
